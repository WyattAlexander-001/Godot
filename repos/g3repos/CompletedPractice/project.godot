[application]

config/name="Learn to Code with Godot: Complete Projects"
run/main_scene="res://DemoSelector.tscn"
config/icon="res://icon.png"

[autoload]

BackgroundInjector="*res://addons/BackgroundInjector/BackgroundInjector.gd"

[debug]

